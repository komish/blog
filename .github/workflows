on: 
  push:
    branches:
    - master
jobs:
  curl:
    runs-on: ubuntu-latest
    steps:
    - name: curl
      uses: wei/curl@master
      with:
        args: -X POST -H "Content-Type: application/json" -d '{ "secret-key": "${{ secrets.WORKFLOW_TARGET }}" }'  ${{ secrets.WORKFLOW_TARGET }}/trigger
